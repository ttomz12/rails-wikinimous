<div class="container">
  <div class="inner-container">
    <body>
      <h1>Check all the articles</h1>
        <h2>
          <% @articles.each do |article| %>
          <li><%= link_to article.title, article_path(article)  %></li>
          <p><%= article.content %></p>
          <% end %>
        </h2>
      <%= link_to "Go home", articles_path %>
    </body>
  </div>
</div>
